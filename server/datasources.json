  "oanda": {
    "name": "oanda",
    "connector": "rest",
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "https://api-fxtrade.oanda.com/v3/instruments/{pair}/candles",
          "headers": {
            "authorization": "Bearer 1fee90dedad1abc90df403870923132c-7ec28839c346f9fc989abbf08c23d99b",
            "accepts": "application/json",
            "content-type": "application/json",
            "Connection": "keep-alive"
          },
          "query": {
            "smooth": true,
            "count": 50,
            "granularity": "{tf}"
          }
        },
        "functions": {
          "cp": [
            "pair",
            "tf"
          ]
        }
      }
    ]
  },
  "orderbook": {
    "name": "orderbook",
    "connector": "rest",
    "gzip": true,
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "https://api-fxtrade.oanda.com/v3/instruments/{pair}/{endpoint}",
          "headers": {
            "Authorization": "Bearer 1fee90dedad1abc90df403870923132c-7ec28839c346f9fc989abbf08c23d99b",
            "Accept-Encoding": "gzip,deflate",
            "accepts": "application/json",
            "content-encoding": "gzip",
            "content-type": "application/json",
            "Connection": "keep-alive"
          },
          "query": {
            "instrument": "{pair}",
            "endpoint": "{endpoint}"
          }
        },
        "functions": {
          "cp": [
            "pair",
            "endpoint"
          ]
        }
      }
    ]
  },
  "oanda1": {
    "name": "oanda1",
    "connector": "rest",
